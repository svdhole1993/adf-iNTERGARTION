					"paramterlinkAzureDataLakeStorageS2_accountKey": {
						"value": "[parameters('paramterlinkAzureDataLakeStorageS2_accountKey')]"
					},
					"paraoutputAzureDataLakeStorage2_accountKey": {
						"value": "[parameters('paraoutputAzureDataLakeStorage2_accountKey')]"
					},
					"step2BlobStorage1_connectionString": {
						"value": "[parameters('step2BlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureKeyVault1azuews3_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1azuews3_properties_typeProperties_baseUrl')]"
					},
					"azuregitlink_properties_typeProperties_url": {
						"value": "[parameters('azuregitlink_properties_typeProperties_url')]"
					},
					"gitstoragedetalake_properties_typeProperties_url": {
						"value": "[parameters('gitstoragedetalake_properties_typeProperties_url')]"
					},
					"paramterlinkAzureDataLakeStorageS2_properties_typeProperties_url": {
						"value": "[parameters('paramterlinkAzureDataLakeStorageS2_properties_typeProperties_url')]"
					},
					"paraoutputAzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('paraoutputAzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"s3akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('s3akv_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sddf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSqlDatabase1_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase1_connectionString')]"
					},
					"AmazonS31_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]"
					},
					"S3_ls_properties_typeProperties_accessKeyId": {
						"value": "[parameters('S3_ls_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sddf_ArmTemplate_0')]"
			]
		},
		{
			"name": "sddf_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sddf_ArmTemplate_1')]"
			]
		}
	]
