{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sddf"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Git_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Git'"
		},
		"LS_AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase1'"
		},
		"MDFLINkS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MDFLINkS'"
		},
		"Setp2sqldatabasesourse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Setp2sqldatabasesourse'"
		},
		"SourceSqlAdventure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SourceSqlAdventure'"
