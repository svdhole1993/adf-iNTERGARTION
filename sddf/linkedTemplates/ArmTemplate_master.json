					},
					"MDFLINkS_connectionString": {
						"value": "[parameters('MDFLINkS_connectionString')]"
					},
					"Setp2sqldatabasesourse_connectionString": {
						"value": "[parameters('Setp2sqldatabasesourse_connectionString')]"
					},
					"SourceSqlAdventure_connectionString": {
